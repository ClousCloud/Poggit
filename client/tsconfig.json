{
	"compilerOptions": {
		"module": "amd",
		"target": "es6",
		"sourceMap": false,
		"outFile": "../gen/client/modules.js",
		"newLine": "LF",
		"charset": "utf8",
		"emitBOM": false,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"allowUnreachableCode": false,
		"preserveConstEnums": true,
		"lib": ["dom"],
		"experimentalDecorators": true,
		"emitDecoratorMetadata": false
	},
	"include": [
		"./**/*.ts",
		"../shared/**/*.ts"
	],
	"compileOnSave": false
}

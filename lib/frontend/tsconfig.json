{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2016",
		"sourceMap": true,
		"newLine": "LF",
		"strict": true,
		"noImplicitReturns": true,
		"preserveConstEnums": true,
		"preserveSymlinks": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	},
	"include": [
		"./src/**/*.ts"
	],
	"compileOnSave": false
}
